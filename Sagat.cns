;======================================================================
;                            Variable Table		
;======================================================================
;			Var(1) : Intro Decider
;			Var(2) : Win Pose Decider
;			Var(3) : Hit Voice Randomizer
;                       Var(4) : Combo Condition
;                       Var(5) : Throw Turn
;                       Var(6) : Damage dampener
;			Var(7) : Tiger Shot Juggle Addition
;			Var(8) : Tiger Shot in play
;			Var(9) : Parry Variable
;			Var(10): A.I Activated
;			Var(11): Trip memory
;			Var(12): Long Jump Stuff
;                       Var(13): Projectile Hit Detect
;                       Var(14): EX Signature
;                       Var(15): EX Projectile Persistency
;                       Var(16): Hyper Projectile HCF Detect
;                       Var(17): Gut Buster Hit Count
;                       Var(18): Tiger Raid Juggle Check
;			Var(19): Tiger Uppercut Juggle Check
;========================Custom HitSparks==============================
;                       Var(30): Hit Detection
;                       Var(35): SparkNo
;                       Var(36): Guard spark
;                       Var(37): Spark X Pos
;                       Var(38): Spark Y Pos
;                       Var(39): Spark Minimum X Dist
;***********************|Floats|***************************************
;                       FVar(0): Landing Corner Push
;                       FVar(1): Falling Vel Y Record
;			FVar(2): Corner Push
;                       FVar(12):Damage Dampener Stuff
;======================================================================
[Data]
life = 1028               ;Amount of life to start with
attack = 100              ;Amount of attack power to start with
defence = 100             ;Amount of defensive power to start with
fall.defence_up = 50      ;Amount to raise defence by on fall
liedown.time = 30         ;How long Sagat lies on the ground
airjuggle = 17            ;Number of juggle points
sparkno = 3               ;Default spark number
guard.sparkno = s1105     ;Default spark number
KO.echo = 0               ;Whether there should be an echo upon death
volume = 225              ;Pretty much self explanitory
IntPersistIndex = 58     ;Variables with this index and above will not
FloatPersistIndex = 40    ;have their values reset to 0 between rounds
;====================================================================

[Size]
xscale = 0.5           ;Horizontal scaling factor.
yscale = 0.5           ;Vertical scaling factor.
ground.back = 20     ;Player width (back, ground)
ground.front = 20    ;Player width (front, ground)
air.back = 16        ;Player width (back, air)
air.front = 16       ;Player width (front, air)
height = 60          ;Height of player (for opponent to jump over)
attack.dist = 160    ;Default attack distance
proj.attack.dist = 90;Default attack distance for projectiles
proj.doscale = 0     ;Set to 1 to scale projectiles too
head.pos = 5, -105   ;Approximate position of head
mid.pos = 0, -65    ;Approximate position of midsection
shadowoffset = 0     ;Number of pixels to vertically offset the shadow
draw.offset = 0,0    ;Player drawing offset in pixels (x, y)

[Velocity]
walk.fwd  = 2.4         ;Walk forward
walk.back = -2.2            ;Walk backward
run.fwd  = 4.5, 0 ;4.3        ;Run forward (x, y)
run.back = -4.5,-3.8      ;Hop backward (x, y)
jump.neu = 0,-9.1 ;-7.6   ;Neutral jumping velocity (x, y)
jump.back = -2.5,-8.7     ;Jump back Speed (x, y)
jump.fwd = 2.7,-8.7       ;Jump forward Speed (x, y)
runjump.back = -3.4,-9    ;Running jump speeds (opt)
runjump.fwd = 3.6,-9      ;.
airjump.neu = 0,-9.4      ;.
airjump.back = -4.55      ;Air jump speeds (opt)
airjump.fwd = 2.5         ;.

[Movement]
airjump.num = 0      ;Number of air jumps allowed (opt)
airjump.height = 32  ;Minimum distance from ground before you can air jump (opt)
yaccel = .49 ;.45        ;Vertical acceleration
stand.friction = .85  ;Friction coefficient when standing
crouch.friction = .82 ;Friction coefficient when crouching

[Quotes]
victory1 = "Your fists lack skill and your spirit is weak. Return to me once you've become a warrior."
Victory2 = "You are not worthy to walk the path of a warrior."
Victory3 = "Before The Emperor of Muay Thai, even the greatest of warriors fall to their knees."
Victory4 = "I admire your courage, but this battle is futile..."
Victory5 = "Is this what passes for a warrior these days? Pathetic!"
Victory6 = "Dry your tears and raise your fists. I'll make a warrior of you today..."
Victory7 = "You'll have to do much better than that if you wish to become a champion."
